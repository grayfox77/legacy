TEXTE:
        DEFB "   HI THERE!!! THIS IS A LITTLE EXAMPLE OF ZOOMED FONTS,"
        DEFB " AND COLOR CYCLING ON THE TOP PART..."
        DEFB "                         "
        DEFB "*"
MULTI2:
        PUSH BC
        IN A,(#FE)
        PUSH AF
        LD A,(PAGERAST)
        OUT (#FE),A
        LD HL,(PTRRAST)
        LD A,128+16
        OUT (#99),A
        LD A,128+17
        OUT (#99),A
        LD C,#9A
        XOR A
        OUT (#9B),A
        LD E,%00100000
        LD A,0
        CALL INTERUPT
        LD A,0
        OUT (#99),A
        LD A,128+18
        OUT (#99),A
        LD A,2
        OUT (#99),A
        LD A,#8F
        OUT (#99),A
        LD D,63
ML1B:   IN A,(#99)
        AND E
        JP NZ,ML1B
        OUT (#9B),A
        OUTI
        OUTI
        OUTI
        OUTI
        
        OUTI
        OUTI
        inc hl  ; can replace by OUTI with an MSX 2 to have one more raster 
        inc hl  ; can replace by OUTI with am MSX 2 to have one more raster
                ; VDP access on TURBO R is too slow to handle this one.
        
ML2B:   IN A,(#99)
        AND E
        JP Z,ML2B
        DEC D
        JP NZ,ML1B
        LD HL,(PTRRAST)
        LD DE,66*8
        ADD HL,DE
        LD (PTRRAST),HL
        LD A,(CPTRAST)
        INC A
        CP 25
        CALL Z,NEWRAST
        LD (CPTRAST),A
        POP AF
        OUT (#FE),A

        POP BC
        RET
PTRRAST:        
        DEFW 0
CPTRAST:
        DEFB 0
CPTT:
        DEFB 0
PLASMA:
        LD HL,(PTRRAST2)
        PUSH BC
        LD A,1
        OUT (#99),A
        LD A,128+16
        OUT (#99),A
        LD B,30
        LD C,#9A
        OTIR
        POP BC
        RET
PLASMAB:                
        LD A,(CPTT)
        INC A
        LD (CPTT),A
        AND %00000011
        CP 0
        RET NZ

        LD HL,(PTRRAST2)
        INC HL
        INC HL
        LD (PTRRAST2),HL
        LD A,(CPTRAST2)
        INC A
        CP 0
        CALL Z,NEWRAST2
        LD (CPTRAST2),A
        
        RET
PTRRAST2:
        DEFW 0
CPTRAST2:
        DEFB 0
NEWRAST:
        LD HL,#8500
        LD (PTRRAST),HL
        LD A,(PAGERAST)
        INC A
        CP 6
        CALL Z,NEWPAGE
        LD (PAGERAST),A
        XOR A
        RET
NEWRAST2:
        LD HL,RASTER
        LD (PTRRAST2),HL
        RET
NEWPAGE:
        LD A,2
        RET
